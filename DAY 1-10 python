#strings
name="harshit"
#string indexing
print(name[1])
print(name[-1])
#string slicing
print(name[0:5])
print(name[0:-2])
print(name[0:])
print(name[2:])
print(name[-4:])
print(name[:-2])
print(name[0:5:1])
print(name[0:5:2])
print(name[-1:0:-1])
#take user input
age=input("enter your age")
print(age)
#take two user input
age=int(input("enter your age"))
print(age)

user_name,age=input("Enter name and age: ").split()
print(user_name)
print(age)
user_name,age=input("Enter name and age: ").split(",")
print(user_name)
print(age)
#len function
len(name)
print(len(name))
#upper, lower, title
print(name.lower())
print(name.upper())
print(name.title())
#find, replace,center method
name="hArShitr"
print(name.find("r"))
print(name.find("r",1))
r_pos=name.find("r")
r_pos_2=name.find("r",r_pos+1)
print(r_pos_2)


name="harshit"
#*harshit*
print(name.center(11,"*"))
print(name.center(15,"*"))
print(name.center(14,"*"))

print(name[2])

name[2]="s"

print(name.replace("h", "H"))
print(name.replace("h", "H",1))
print(name)


